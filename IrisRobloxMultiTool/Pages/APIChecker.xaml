                <TextBlock Foreground="Orange" Name="MarketplaceApiPing" TextDecorations="Underline" FontWeight="Bold" Text="85ms"/>
            </StackPanel>
        </Label>
        <Border ClipToBounds="True" CornerRadius="6" Background="White" Margin="28,160,28,184" Opacity="0.05" />
