<Page
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      x:Class="IrisRobloxMultiTool.Pages.ApiChecker"
      mc:Ignorable="d" 
      d:DesignHeight="380" d:DesignWidth="668"
      Title="APIChecker" Loaded="Page_Loaded">

    <Grid>
        <Border ClipToBounds="True" CornerRadius="6" Background="White" Margin="28,-4,28,348" Opacity="0.05"/>
        <Label Margin="28,-4,28,348" HorizontalAlignment="Center" Opacity="100" VerticalAlignment="Center" FontSize="14px">
            <StackPanel Orientation="Horizontal">
                <TextBlock Foreground="White" FontWeight="Bold" Text="Asset Delivery API is "/>
                <TextBlock Foreground="Lime" Name="AssetDeliveryOnlineStatus" TextDecorations="Underline" FontWeight="Bold" Text="Online"/>
                <TextBlock Foreground="White" FontWeight="Bold" Text=" with a speed of "/>
                <TextBlock Foreground="Orange" Name="AssetDeliveryPing" TextDecorations="Underline" FontWeight="Bold" Text="85ms"/>
            </StackPanel>
        </Label>
        <Border ClipToBounds="True" CornerRadius="6" Background="White" Margin="28,37,28,307" Opacity="0.05" />
        <Label Margin="28,37,28,307" HorizontalAlignment="Center" Opacity="100" VerticalAlignment="Center" FontSize="14px">
